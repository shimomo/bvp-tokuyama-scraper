{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "bvp/tokuyama-scraper",
    "description": "The BVP Tokuyama Scraper package for Boatrace.",
    "keywords": [
        "boatrace",
        "japan",
        "japanese",
        "library",
        "scraper"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "shimomo",
            "email": "bvp@shimomo.net"
        }
    ],
    "support": {
        "issues": "https://github.com/shimomo/bvp-tokuyama-scraper/issues",
        "source": "https://github.com/shimomo/bvp-tokuyama-scraper"
    },
    "require": {
        "php": "^8.2",
        "bvp/scraper-core": "^4.0",
        "nesbot/carbon": "^2.63 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1 || ^11.1 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "BVP\\TokuyamaScraper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BVP\\TokuyamaScraper\\Tests\\": "tests/"
        }
    }
}
